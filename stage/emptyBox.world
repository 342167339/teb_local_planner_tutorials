# include our robot and obstacle definitions
include "robots/myRobot.inc"
include "robots/myObstacle.inc"

## Simulation settings
resolution 0.02
interval_sim 100  # simulation timestep in milliseconds

## Load a static map
model
(
  # most maps will need a bounding box
  name "emptyBox"
  bitmap "../maps/emptyBox.png"
  size [ 6.0 6.0 2.0 ]
  pose [ 0.0 0.0 0.0 0.0 ]
  laser_return 1
  color "gray30"
)

# throw in a robot
myrobot
(
  pose [ -1.0 0.0 0.0 0.0 ]
  name "myRobot"
)

myobstacle
(
  pose [ 1.0 1.0 0.0 0.0 ]
  name "obstacle"
)